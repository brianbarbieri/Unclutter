{
    "name": "Unclutter",
    "description": "Chrome extension that redirects articles wo outline for an uncluttered read.",
    "version": "1.0",
    "manifest_version": 2,  
    "background": {
        "scripts":["background.js"]
      },
      "content_scripts": [
        {
            "matches": ["*://*.medium.com/*", "*://*.towardsdatascience.com/*"],
            "js": ["contentscript.js"]
        }
      ],
      "permissions": [
        "tabs"
      ],
      "run_at": "document_start"
}